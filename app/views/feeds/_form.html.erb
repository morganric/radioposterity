<%= simple_form_for(@feed) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :user_id, as: :hidden %>
    <%= f.input :title, as: :hidden %>
    <%= f.input :description, as: :hidden %>
    <%= f.input :category, as: :hidden %>
    <%= f.input :link, as: :hidden %>
    <%= f.input :email, as: :hidden %>
    <%= f.input :image, as: :hidden %>
  </div>

  <br/>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
